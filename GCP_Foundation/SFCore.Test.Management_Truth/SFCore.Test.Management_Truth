  name = "rule-38"
  network = "${var.vpc}"
  project = "${var.project}"
  priority= 10000
  tcp_ports = ["4194","10248-10250","10255"]
  udp_ports = []
  target_tags = ["sfcore-test-perimeter-loadbalancer"]
  source_tags = ["sfcore-test-management-truth"]
  source_ranges= ["20.32.0.0/12"]
} 
